@model IEnumerable<Silicus.Finder.Models.DataObjects.SkillSet>

@{
    var currenProjectId = ViewData["id"];
}

<p>
    <h2>Project SkillSets</h2>
</p>
<p>
    @Html.ActionLink("Add Skills", "AddSkillsToProject", "Projects", new { id = currenProjectId }, null)
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SkillSetId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SkillSetId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
    </tr>
}
</table>