@model IEnumerable<Silicus.Finder.Web.ViewModel.EmployeeNameViewModel>
@{
    ViewBag.Title = "SearchEmployeeByName";
}

<h2>SearchEmployeeByName</h2>
<table class="table">
<tr>
    <th>
        @Html.DisplayNameFor(model => model.FirstName)
    </th>
    <th></th>
</tr>

@foreach (var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FirstName)

        </td>
        <td>
           
            @Html.ActionLink("Details", "Details", new { id = item.EmployeeId }) |
           
        </td>
    </tr>
}

</table>


